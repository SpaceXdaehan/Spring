package com.itwill.spring3.repository.member;

public enum Role {
    USER("ROLE_USER", "USER"),
    ADMIN("ROLD_ADMIN", "ADMIN");
    
    private final String key;
    private final String name;
    
    
    // 생성자
    Role(String key, String name) {
        this.key = key;
        this.name = name;
    } 
    
    
    // getter메서드
    public String getKey() {
        return this.key;
    }

}
